{
  "name": "shopozor-consumer-frontend",
  "version": "0.0.1",
  "description": "le magasin local suisse en ligne",
  "productName": "La Budzonnerie",
  "cordovaId": "org.cordova.quasar.app",
  "author": "Softozor",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue src",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "test:unit": "jest --updateSnapshot",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\"",
    "test:unit:ui": "majestic",
    "build": "quasar build",
    "start": "quasar serve dist/spa-mat",
    "start:dev": "quasar dev",
    "e2e": "cypress run"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "quasar": "^1.0.0-beta.0",
    "apollo-cache-inmemory": "^1.4.2",
    "apollo-client": "^2.4.12",
    "apollo-link": "^1.2.8",
    "apollo-link-http": "^1.5.11",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1",
    "node-fetch": "^2.3.0",
    "vue-apollo": "^3.0.0-beta.27",
    "vue-i18n": "^8.8.0"
  },
  "devDependencies": {
    "@quasar/app": "^1.0.0-beta.0",
    "@quasar/quasar-app-extension-testing": "^1.0.0-rc.6",
    "@quasar/quasar-app-extension-testing-unit-jest": "^1.0.0-beta.23",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.10.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "strip-ansi": "=3.0.1",
    "wallaby-vue-compiler": "^1.0.3",
    "majestic": "^1.2.24",
    "cypress": "^3.1.5",
    "cypress-cucumber-preprocessor": "^1.11.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "jwt-decode": "^2.2.0",
    "moment": "^2.24.0",
    "quasar-cli": "^0.17.23",
    "vue-template-compiler": "^2.5.22"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11",
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "resolutions": {
    "ajv": "6.8.1"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/softozor/shopozor-consumer-frontend.git"
  },
  "license": "ISC",
  "homepage": "https://bitbucket.org/softozor/shopozor-consumer-frontend#readme",
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  }
}
